{
    "name": "trivago/tas-sdk-php",
    "description": "trivago Affiliate Suite SDK for PHP allows easy access the trivago Affiliate Suite API",
    "require": {
        "ext-curl": "*",
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "trivago",
            "homepage": "https://github.com/trivago/tas-sdk-php/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Trivago\\Tas\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-quick": "phpunit --no-coverage"
    },
    "config": {
        "sort-packages": true
    }
}
